{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.css"
    ],
    "critical" : [
      "./styles/common/_index.css"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "publicPath": "/app/themes/__project-name__",
  "devUrl": "https://__project-name__.test",
  "proxyUrl": "https://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/**/*.php"
  ]
}
