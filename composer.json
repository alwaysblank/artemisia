{
  "name": "benspants/artemisia",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "Tweaked version of Murmur's fork of roots/sage.",
  "homepage": "https://roots.io/sage/",
  "authors": [
    {
      "name": "Ben Martinez-Bateman",
      "email": "ben@murmurcreative.com",
      "homepage": "https://alwaysblank.org"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    },
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "QWp6t",
      "email": "hi@qwp6t.me",
      "homepage": "https://github.com/qwp6t"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "repositories" : {
    "murmurcreative/better-excerpt": {
      "type": "vcs",
      "url": "git@bitbucket.org:murmurcreative/better-excerpt.git"
    },
    "murmurcreative/cabinet": {
      "type": "vcs",
      "url": "git@bitbucket.org:murmurcreative/cabinet.git"
    },
    "murmurcreative/image-tools": {
      "type": "vcs",
      "url": "git@bitbucket.org:murmurcreative/image-tools.git"
    },
    "alwaysblank/mnemosyne": {
      "type": "vcs",
      "url": "https://github.com/alwaysblank/mnemosyne.git"
    },
    "murmurcreative/use-fields": {
      "type": "vcs",
      "url": "git@bitbucket.org:murmurcreative/use-fields.git"
    },
    "zenodorus/core": {
      "type": "vcs",
      "url": "https://github.com/zenodorus-tools/core.git"
    },
    "zenodorus/arrays": {
        "type": "vcs",
        "url": "https://github.com/zenodorus-tools/arrays.git"
    },
    "zenodorus/strings": {
        "type": "vcs",
        "url": "https://github.com/zenodorus-tools/strings.git"
    }
  },
  "require": {
    "php": ">=7",
    "composer/installers": "~1.0",
    "illuminate/support": "~5.4",
    "roots/sage-lib": "~9.0.0-beta.4",
    "soberwp/controller": "~9.0.0-beta.4",
    "zenodorus/strings": "dev-master",
    "zenodorus/arrays": "dev-master"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8.0",
    "roots/sage-installer": "~1.3"
  },
  "scripts": {
    "test": ["phpcs"]
  }
}
