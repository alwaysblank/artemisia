{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.css"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "watch": [
    "templates/**/*.php",
    "src/**/*.php"
  ],
  "publicPath": "/app/themes/__project-name__",
  "devUrl": "https://__project-name__.dev",
  "proxyUrl": "https://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "browsers": [
    "last 2 versions",
    "not ie < 11"
  ]
}
