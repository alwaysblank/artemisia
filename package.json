{
  "name": "artemisia",
  "version": "0.9-beta.4",
  "author": "Ben Martinez-Bateman <ben@murmurcreative.com>",
  "homepage": "https://bitbucket.org/benspants/artemisia/",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:benspants/artemisia.git"
  },
  "bugs": {
    "url": "https://bitbucket.org/benspants/artemisia/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "browserslist": [
    "last 2 versions",
    "not ie < 11"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "eslint:recommended",
    "globals": {
      "wp": true
    },
    "env": {
      "node": true,
      "es6": true,
      "amd": true,
      "browser": true,
      "jquery": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "globalReturn": true,
        "generators": false,
        "objectLiteralDuplicateProperties": false,
        "experimentalObjectRestSpread": true
      },
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "plugins": [
      "import"
    ],
    "settings": {
      "import/core-modules": [],
      "import/ignore": [
        "node_modules",
        "\\.(coffee|scss|css|less|hbs|svg|json)$"
      ]
    },
    "rules": {
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-empty-source": null
    }
  },
  "scripts": {
    "build": "webpack --progress --config resources/assets/build/webpack.config.js",
    "build:production": "webpack --progress -p --config resources/assets/build/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config resources/assets/build/webpack.config.js",
    "start": "webpack --hide-modules --watch --config resources/assets/build/webpack.config.js",
    "rmdist": "rimraf dist",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint resources/assets/scripts resources/assets/build",
    "lint:styles": "stylelint resources/assets/styles/**/*.{css,sass,scss,sss,less}",
    "test": "npm run -s lint"
  },
  "engines": {
    "node": ">= 6.9.4"
  },
  "devDependencies": {
    "autoprefixer": "~7.1",
    "browser-sync": "~2.18",
    "browsersync-webpack-plugin": "^0.6.0",
    "bs-html-injector": "~3.0",
    "buble-loader": "^0.4.1",
    "cache-loader": "~1.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-globs-webpack-plugin": "^0.2.0",
    "css-loader": "^0.28.4",
    "cssnano": "~4.0.0-rc.1",
    "eslint": "~4.9",
    "eslint-loader": "~1.9",
    "eslint-plugin-compat": "^2.0",
    "eslint-plugin-import": "~2.8",
    "extract-text-webpack-plugin": "~3.0",
    "file-loader": "^1.1",
    "imagemin-mozjpeg": "~6.0",
    "imagemin-webpack-plugin": "~1.5.0-beta.0",
    "import-glob": "~1.5",
    "minimist": "^1.2.0",
    "pleeease-filters": "^4.0.0",
    "postcss": "6.0.13",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-color-rgba-fallback": "^3.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.0.1",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-import": "^11.0",
    "postcss-loader": "~2.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-normalize": "^4.0.0",
    "postcss-nth-child-fix": "^2.0.0",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-safe-parser": "~3.0",
    "postcss-selector-not": "^3.0.1",
    "qs": "^6.3.0",
    "resolve-url-loader": "~2.1",
    "rimraf": "~2.6",
    "style-loader": "^0.19.0",
    "stylelint": "~8.2",
    "stylelint-config-standard": "~17.0",
    "stylelint-no-unsupported-browser-features": "^1.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.6",
    "webpack": "~3.8",
    "webpack-assets-manifest": "^1.0",
    "webpack-dev-middleware": "~1.12",
    "webpack-hot-middleware": "~2.20",
    "webpack-merge": "~4.1",
    "yargs": "~10.0"
  },
  "dependencies": {
    "friendly-errors-webpack-plugin": "^1.6.1",
    "jquery": "1.12.4 - 3"
  }
}
